<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bottom_navigation_graph"
    app:startDestination="@id/feed_fragment">


    <fragment
        android:id="@+id/feed_fragment"
        android:name="com.example.tourroom.ui.feed.feed_fragment"
        android:label="feed_fragment"
        tools:layout="@layout/feed_fragment" />
    <fragment
        android:id="@+id/place_fragment"
        android:name="com.example.tourroom.ui.place.place_fragment"
        android:label="place_fragment"
        tools:layout="@layout/place_fragment" >
        <action
            android:id="@+id/action_place_fragment_to_testfragment2"
            app:destination="@id/testfragment2" />
    </fragment>
    <fragment
        android:id="@+id/group_fragment"
        android:name="com.example.tourroom.ui.group.group_fragment"
        android:label="group_fragment"
        tools:layout="@layout/group_fragment" />
    <fragment
        android:id="@+id/search_fragment"
        android:name="com.example.tourroom.ui.search.search_fragment"
        android:label="search_fragment"
        tools:layout="@layout/search_fragment" />
    <fragment
        android:id="@+id/notification_fragment"
        android:name="com.example.tourroom.ui.noti_fication.notification_fragment"
        android:label="notification_fragment"
        tools:layout="@layout/notification_fragment" />
    <fragment
        android:id="@+id/testfragment2"
        android:name="com.example.tourroom.testfragment2"
        android:label="fragment_testfragment2"
        tools:layout="@layout/fragment_testfragment2" />
</navigation>