<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_nav_graph"
    app:startDestination="@id/nav_Home">

    <fragment
        android:id="@+id/nav_Home"
        android:name="com.example.tourroom.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home"/>

    <fragment
        android:id="@+id/nav_Profile"
        android:name="com.example.tourroom.ui.profile.profileFragment"
        android:label="@string/menu_Profile"
        tools:layout="@layout/fragment_profile">
        <action
            android:id="@+id/action_nav_Profile_to_edit_profile_fragment"
            app:destination="@id/edit_profile_fragment" />
    </fragment>

    <fragment
        android:id="@+id/nav_visited_places"
        android:name="com.example.tourroom.ui.Visit_places.visit_placesFragment"
        android:label="@string/menu_visited_places"
        tools:layout="@layout/fragment_visitedplaces" />
    <fragment
        android:id="@+id/about_fragment"
        android:name="com.example.tourroom.ui.about.about_fragment"
        android:label="@string/menu_about"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/group_info_fragment"
        android:name="com.example.tourroom.ui.group.Group_info_fragment"
        android:label="Group Info"
        tools:layout="@layout/group_info_fragment" />
    <fragment
        android:id="@+id/edit_profile_fragment"
        android:name="com.example.tourroom.ui.profile.edit_profile_fragment"
        android:label="Edit Profile"
        tools:layout="@layout/edit_profile_fragment" />
    <fragment
        android:id="@+id/place_info_fragment"
        android:name="com.example.tourroom.ui.place.place_info_fragment"
        android:label="Place Info"
        tools:layout="@layout/place_info_fragment" />
    <fragment
        android:id="@+id/add_new_place_fragment"
        android:name="com.example.tourroom.ui.place.add_new_place_fragment"
        android:label="Add a new place"
        tools:layout="@layout/add_new_place_fragment" />
    <fragment
        android:id="@+id/create_group_fragment"
        android:name="com.example.tourroom.ui.group.create_group_fragment"
        android:label="Create a new group"
        tools:layout="@layout/create_group_fragment" />

</navigation>